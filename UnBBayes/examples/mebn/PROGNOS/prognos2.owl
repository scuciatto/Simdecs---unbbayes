    <owl:disjointWith rdf:resource="#BooleanRVState"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#CategoricalRVState"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#MetaEntity"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#OVariable"/>
