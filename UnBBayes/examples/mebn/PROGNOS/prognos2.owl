      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasMFrag"/>
        </owl:onProperty>
        <owl:allValuesFrom rdf:resource="#MFrag"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasMFrag"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="#MFrag"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#CondRelationship">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:ID="hasParentState"/>
        </owl:onProperty>
