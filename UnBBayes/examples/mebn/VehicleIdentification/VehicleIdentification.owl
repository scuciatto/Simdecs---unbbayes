    <hasDeclaration rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >if any obj have ( ObjectType = Tracked &amp; Speed = Stationary) [
   if any rgn have ( TerrainType = Road ) [
      Stationary = .85,
      Slow = .1,
      Medium = .05,
      Fast = 0,
      VeryFast = 0
   ] else if any rgn have ( TerrainType = OffRoad ) [
      Stationary = .85,
      Slow = .15,
      Medium = 0,
      Fast = 0,
      VeryFast = 0
   ] else [
      Stationary = .9,
      Slow = .1,
      Medium = 0,
      Fast = 0,
      VeryFast = 0
   ] 
] else if any obj have ( ObjectType = Tracked &amp; Speed = Slow) [
   if any rgn have ( TerrainType = Road ) [
      Stationary = .05,
      Slow = .85,
      Medium = .1,
      Fast = 0,
      VeryFast = 0
   ] else if any rgn have ( TerrainType = OffRoad ) [
      Stationary = .05,
      Slow = .9,
      Medium = .05,
      Fast = 0,
      VeryFast = 0
   ] else [
      Stationary = .1,
      Slow = .85,
      Medium = .05,
      Fast = 0,
      VeryFast = 0
   ] 
] else if any obj have ( ObjectType = Tracked &amp; Speed = Medium) [
   if any rgn have ( TerrainType = Road ) [
      Stationary = 0,
      Slow = .1,
      Medium = .85,
      Fast = .05,
      VeryFast = 0
   ] else if any rgn have ( TerrainType = OffRoad ) [
      Stationary = 0,
      Slow = .15,
      Medium = .8,
      Fast = .05,
      VeryFast = 0
   ] else [
      Stationary = .05,
      Slow = .1,
      Medium = .8,
      Fast = .05,
      VeryFast = 0
   ] 
] else if any obj have ( ObjectType = Tracked &amp; Speed = Fast) [
   if any rgn have ( TerrainType = Road ) [
      Stationary = 0,
      Slow = .05,
      Medium = .1,
      Fast = .8,
      VeryFast = .05
   ] else if any rgn have ( TerrainType = OffRoad ) [
